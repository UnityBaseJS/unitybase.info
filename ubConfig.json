{
	"httpServer": {
		"host": "localhost",
		"port": "80",
		"threadPoolSize": 2,
		"inetPub": ".\\public",
		"headersPostprocessors": [
			{
				"name": "allStatic",
				"location": "\\.(js|html|css)$",
				"usePreparedGzip": true,
				"addHeader": "Vary: Accept-Encoding",
				"useETag": true
			},
			{
				"name": "images",
				"location": "\\.(ico|txt|gif|jpg|png|jpeg)$",
				"expires": 25920000
			}
		],
		"watchFileChanges": {
			"enabled": false,
			"hashingStrategy": "ModifyDate"
		}
	},
	"logging": {
		"levels": [
			"Exception",
			"ExceptionOS",
			"Memory",
			"StackTrace",
			"Fail",
			"Warning",
			"Error"
		],
		"path": ".\\_logs",
		"rotationSizeInMB": 100
	},
	"uiSettings": {
		"adminUI": {
			"themeName": "UBGrayTheme"
		}
	}
}